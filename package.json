{
  "name": "z-waypoints",
  "version": "1.0.0",
  "description": "Waypoints for Vue.js and applications requiring ES6 import syntax. The core functionality is cloned from https://github.com/imakewebthings/waypoints",
  "main": "./build/index.js",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "html-webpack-plugin": "^3.2.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress --mode production",
    "devBuild": "cross-env NODE_ENV=development webpack --progress --mode development --config=webpack.config.dev.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --config=webpack.config.dev.js",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kalesh13/z-waypoints.git"
  },
  "keywords": [
    "waypoints",
    "vuejs",
    "es6",
    "scroll",
    "handler",
    "javascript",
    "jquery"
  ],
  "author": "Kalesh Kaladharan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kalesh13/z-waypoints/issues"
  },
  "homepage": "https://github.com/kalesh13/z-waypoints#readme"
}
